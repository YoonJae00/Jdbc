<?xml version="1.0" encoding="UTF-8" standalone = "no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!-- 주석 부분
        DOCTYPE : 문서의 형태 xml
 -->
<properties>
    <entry key="selectEmpByFamilyName">
        SELECT *
        FROM EMPLOYEE
        WHERE EMP_NAME LIKE CONCAT(?,'%')
    </entry>
</properties>